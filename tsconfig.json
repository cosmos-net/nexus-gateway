{
  "compilerOptions": {
    "moduleResolution": "node",
    "module": "CommonJS",
    "strict": true,
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2017",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": false,
    "rootDir": "./",
    "noEmit": false,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "paths": {
      "@core": [
        "src/contexts/core"
      ],
      "@core/*": [
        "src/contexts/core/*"
      ],
      "@common": [
        "src/contexts/common"
      ],
      "@common/*": [
        "src/contexts/common/*"
      ],
      "@hades": [
        "src/contexts/hades"
      ],
      "@hades/*": [
        "src/contexts/hades/*"
      ],
      "@muses": [
        "src/contexts/muses"
      ],
      "@muses/*": [
        "src/contexts/muses/*"
      ],
      "@cerberus": [
        "src/contexts/cerberus"
      ],
      "@cerberus/*": [
        "src/contexts/cerberus/*"
      ],
      "@health": [
        "src/contexts/health"
      ],
      "@health/*": [
        "src/contexts/health/*"
      ],
    }
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules"]
}
